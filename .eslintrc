{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "jest": true,
    "browser": true
  },
  "plugins": ["import", "react"],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "react-app"
  ],
  "rules": {
    "prefer-arrow-callback": 2,
    "react/display-name": 0,
    "semi": [2, "never"]
  }
}
